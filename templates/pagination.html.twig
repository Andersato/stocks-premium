<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-end">
        <li class="page-item {% if page.page == 1 %}disabled{% endif %}">
            <a class="page-link" {% if page.page != 1 %}href="{{ path('screener', app.request.query.all|merge({page: page.page - 1})) }}"{% endif %}>Previous</a>
        </li>
        {% if page.page - 2 > 0 %}
            <li class="page-item"><a class="page-link" href="{{ path('screener', app.request.query.all|merge({ page: page.page - 2 })) }}">{{ page.page - 2 }}</a></li>
        {% endif %}
        {% if page.page - 1 > 0 %}
            <li class="page-item"><a class="page-link" href="{{ path('screener', app.request.query.all|merge({ page: page.page - 1 })) }}">{{ page.page - 1 }}</a></li>
        {% endif %}
        <li class="page-item"><a class="page-link active">{{ page.page }}</a></li>
        <li class="page-item"><a class="page-link" href="{{ path('screener', app.request.query.all|merge({ page: page.page+1 })) }}">{{ page.page + 1 }}</a></li>
        <li class="page-item"><a class="page-link" href="{{ path('screener', app.request.query.all|merge({ page: page.page+2 })) }}">{{ page.page + 2 }}</a></li>
        <li class="page-item">
            <a class="page-link" href="{{ path('screener', app.request.query.all|merge({page: page.page+1})) }}">Next</a>
        </li>
    </ul>
</nav>