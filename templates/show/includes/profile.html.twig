{% trans_default_domain 'show' %}

<div class="col-md-3">
    <h2>{{ 'titles.profile'|trans }}</h2>
    <div class="table-responsive">
        <table class="table table-dark table-borderless table-sm">
            <tbody>
            <tr>
                <td class="table-dark">{{ 'table.name'|trans }}</td>
                <td class="table-success">{{ informationStock.stock.name }} </td>
            </tr>
            <tr>
                <td class="table-dark">{{ 'table.sector'|trans }}</td>
                <td class="table-success">{{ informationStock.stock.sector }} </td>
            </tr>
            <tr>
                <td class="table-dark">{{ 'table.industry'|trans }}</td>
                <td class="table-success">{{ informationStock.stock.industry }} </td>
            </tr>
            <tr>
                <td class="table-dark">{{ 'table.index'|trans }}</td>
                <td class="table-success">{{ informationStock.stock.indexName }} </td>
            </tr>
            <tr>
                <td class="table-dark">{{ 'table.marketCap'|trans }}</td>
                <td class="table-success">{{ informationStock.marketCap }} (MM)</td>
            </tr>
            <tr>
                <td>{{ 'table.employees'|trans }}</td>
                <td class="table-success">{{ informationStock.employees }}</td>
            </tr>
            </tbody>
        </table>
    </div>
</div>